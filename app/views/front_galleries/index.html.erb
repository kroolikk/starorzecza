Galerie:<br>===========================================<br>
<% @galleries.each do |gallery| %>
  <%= link_to front_gallery_url(gallery) do %>
    <%= gallery.name %>-------------<br>
    <% gallery.photos.each do |photo| %>
      <%= image_tag photo.photo.thumb.url %><br>
      <%= photo.title %><br>
      <%= raw photo.description %><br><br>
    <% end %>
  <% end %>
<% end %>
